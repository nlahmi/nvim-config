{
    "$schema": "https://raw.githubusercontent.com/mfussenegger/dapconfig-schema/master/dapconfig-schema.json",
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug: main.py",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "python": "${workspaceFolder}/.venv/bin/python",
            "console": "integratedTerminal",
            "redirectOutput": true,
            "envFile": ".env"
        }
    ]
}
